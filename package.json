{
  "name": "telegram-bot-python",
  "version": "1.0.0",
  "description": "Telegram bot with SafeCheck, Tron, and Rapira integrations",
  "main": "dist/bot.js",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "node dist/bot.js",
    "dev": "ts-node src/bot.ts",
    "watch": "nodemon --exec ts-node src/bot.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "telegram",
    "bot",
    "tron",
    "rapira",
    "safecheck"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.15.6",
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "telegraf": "^4.15.6",
    "tronweb": "^5.3.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2"
  }
}
